    <div class = 'card'>
        <img id="cesarPic" alt="CM in front of a wall" onclick = 'flipCard()' src = 'imgs/cesar.jpg'> 
       
    </div>
    
    
   

    <script>

function flipCard(){
    let Cesar = document.getElementById('cesarPic');
    if (Cesar.src.match('imgs/cesar.jpg')){
        Cesar.src = ('imgs/cesarbio.png')}
        else {
            Cesar.src = ('imgs/cesar.jpg');
        }
    }





    </script>
   
</body>